{
  "name": "buzzcord",
  "version": "0.1.0",
  "description": "BuzzCord \u2014 FOSS social notifications bot for Discord (RSS, YouTube)",
  "type": "module",
  "private": false,
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "deploy:commands": "tsx src/deploy.ts",
    "test": "node --test",
    "lint": "eslint . --ext .ts",
    "release": "echo release"
  },
  "dependencies": {
    "better-sqlite3": "^11.5.0",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2",
    "@typescript-eslint/parser": "^8.6.0",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "eslint": "^9.9.0"
  }
}